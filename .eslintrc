{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "strict": 0,
    "no-console": 0,
    "new-cap": 0,
    "func-names": 0,
    "arrow-body-style": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "no-underscore-dangle": 0
  }
}