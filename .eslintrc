{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "strict": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "no-unused-vars": 0,
    "prefer-const": 0,
    "no-param-reassign": 0,
    "prefer-template": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "max-len": 0
  }
}