{
  "name": "ng6-fullstack",
  "version": "0.0.0",
  "description": "A seed project for Angular 1.x with Express server",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/app.js",
    "dev": "cross-env NODE_ENV=development nodemon ./server/app.js --watch ./server",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "ncp ./server ./dist/ && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "test": "cross-env NODE_ENV=test mocha ./server/test/**/*.test.js"
  },
  "keywords": [
    "angular",
    "express",
    "webpack"
  ],
  "author": "Chun-Kai Wang <chunkai1312@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.3",
    "angular-animate": "^1.5.3",
    "angular-aria": "^1.5.3",
    "angular-material": "^1.0.7",
    "angular-messages": "^1.5.3",
    "api-error-handler": "^1.0.0",
    "body-parser": "^1.15.0",
    "co-express": "^1.2.1",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "http-errors": "^1.4.0",
    "lodash": "^4.10.0",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.12",
    "morgan": "^1.7.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "mocha": "^2.4.5",
    "ncp": "^2.0.0",
    "ng-annotate-loader": "^0.1.0",
    "ngcomponentrouter": "^2.1.0",
    "nodemon": "^1.9.1",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "supertest": "^1.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
