{
  "name": "ng6-fullstack",
  "version": "1.0.0",
  "description": "A seed project for Angular 1.x with Express server",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist/app.js",
    "dev": "cross-env NODE_ENV=development node ./src/index.js",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "ncp ./src/server ./dist/ && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "test": "cross-env NODE_ENV=test mocha ./src/server/test/**/*.test.js",
    "generator:component": "node ./generator"
  },
  "keywords": [
    "angular",
    "express",
    "webpack"
  ],
  "author": "Chun-Kai Wang <chunkai1312@gmail.com>",
  "license": "MIT",
